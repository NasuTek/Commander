#define VER_FILEVERSION             5,0,0,0
#define VER_FILEVERSION_STR         "5.0.0\0"

#define VER_PRODUCTVERSION          5,0,0,0
#define VER_PRODUCTVERSION_STR      "5.0.0\0"

#ifndef DEBUG
#define VER_DEBUG                   0
#else
#define VER_DEBUG                   VS_FF_DEBUG
#endif

1 ICON "img/bsc.ico"

1 VERSIONINFO
