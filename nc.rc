        BLOCK "040904E4"
        BEGIN
            VALUE "CompanyName",      "NasuTek Enterprises"
			VALUE "FileDescription",  "NasuTek Commander Main Executable"
            VALUE "FileVersion",      VER_FILEVERSION_STR
            VALUE "InternalName",     "nc"
            VALUE "LegalCopyright",   "Copyright © 2011 NasuTek Enterprises"
            VALUE "OriginalFilename", "nc.exe"
            VALUE "ProductName",      "NasuTek Commander"
            VALUE "ProductVersion",   VER_PRODUCTVERSION_STR
        END
    END

    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1252
    END
END
